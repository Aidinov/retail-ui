  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "creevey:ui",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["--node-arg", "--inspect=9229"],
      "args": ["creevey", "--ui"],
      "autoAttachChildProcesses": true,
      "cwd": "${workspaceFolder}/packages/react-ui-selenium",
      "sourceMaps": true,
      "port": 9229
    }
  ]
}
